<?xml version='1.0' encoding='UTF-8'?>
    <feed
        xmlns='http://www.w3.org/2005/Atom'
        xmlns:gd='http://schemas.google.com/g/2005'
        xmlns:openSearch='http://a9.com/-/spec/opensearch/1.1/'
        xmlns:yt='{{url}}/schemas/2007'
        xmlns:media='http://search.yahoo.com/mrss/' gd:etag='W/&quot;DkcBQ3g-fip7I2A9XRRXEUw.&quot;'>
        <id>tag:youtube.com,2008:channels</id>
        <updated>2015-02-16T19:14:12.656Z</updated>
        <category scheme='http://schemas.google.com/g/2005#kind' term='{{url}}/schemas/2007#channel'/>
        <title>Channels matching: webauditors</title>
        <logo>http://www.gstatic.com/youtube/img/logo.png</logo>
        <link rel='http://schemas.google.com/g/2006#spellcorrection' type='application/atom+xml' href='{{url}}/feeds/api/channels?q=web+auditors&amp;start-index=1&amp;max-results=1&amp;oi=spell&amp;spell=1&amp;v=2' title='web auditors'/>
        <link rel='http://schemas.google.com/g/2005#feed' type='application/atom+xml' href='{{url}}/feeds/api/channels?v=2'/>
        <link rel='http://schemas.google.com/g/2005#batch' type='application/atom+xml' href='{{url}}/feeds/api/channels/batch?v=2'/>
        <link rel='self' type='application/atom+xml' href='{{url}}/feeds/api/channels?q=webauditors&amp;start-index=1&amp;max-results=1&amp;v=2'/>
        <link rel='service' type='application/atomsvc+xml' href='{{url}}/feeds/api/channels?alt=atom-service&amp;v=2'/>
		{% if next_page %}
        	<link rel='next' type='application/atom+xml' href='{{next_page}}'/>
		{% endif %}
        <author>
            <name>YouTube</name>
            <uri>http://www.youtube.com/</uri>
        </author>
    <generator version='2.1' uri='{{url}}/'>YouTube data API</generator>
    <openSearch:totalResults>1</openSearch:totalResults>
    <openSearch:startIndex>1</openSearch:startIndex>
    <openSearch:itemsPerPage>1</openSearch:itemsPerPage>
{% if data %}
	{% for info in data %}
		<entry gd:etag='W/&quot;YDwqeyM.&quot;'>
			<id>tag:youtube.com,2008:playlist:{{info['videoId']}}:{{info['videoId']}}</id>
			<published>{{'published' in info and unix(info['published']) or unix('0')}}</published>
			<updated>{{'published' in info and unix(info['published']) or unix('0')}}</updated>
			<category scheme='https://schemas.google.com/g/2005#kind' term='https://gdata.youtube.com/schemas/1970#video'/>
			<category scheme='https://gdata.youtube.com/schemas/1970/categories.cat' term='Howto' label='Howto &amp; Style'/>
			<title>{{info['title']}}</title>
			<content type='application/x-shockwave-flash' src="https://www.youtube.com/v/{{info['videoId']}}?version=3&amp;f=playlists&amp;app=youtube_gdata"/>
			<link rel='alternate' type='text/html' href="https://www.youtube.com/watch?v={{info['videoId']}}&amp;feature=youtube_gdata"/>
			<link rel='http://gdata.youtube.com/schemas/2007#video.related' type='application/atom+xml' href="https://gdata.youtube.com/feeds/api/videos/{{info['videoId']}}/related"/>
			<link rel='https://gdata.youtube.com/schemas/1970#mobile' type='text/html' href="https://m.youtube.com/details?v={{info['videoId']}}"/>
			<link rel='https://gdata.youtube.com/schemas/1970#uploader' type='application/atom+xml' href="https://gdata.youtube.com/feeds/api/users/{{info['authorId']}}?v=2"/>
			<link rel='related' type='application/atom+xml' href="https://gdata.youtube.com/feeds/api/videos/{{info['videoId']}}?v=2"/>
			<link rel='self' type='application/atom+xml' href='https://gdata.youtube.com/feeds/api/playlists/8E2186857EE27746/PLyl9mKRbpNIpJC5B8qpcgKX8v8NI62Jho?v=2'/>
			<author>
				<name>{{info['author']}}</name>
				<uri>https://gdata.youtube.com/feeds/api/users/{{info['authorId']}}</uri>
				<yt:userId>{{info['authorId']}}</yt:userId>
			</author>
			<yt:accessControl action='comment' permission='allowed'/>
			<yt:accessControl action='commentVote' permission='allowed'/>
			<yt:accessControl action='videoRespond' permission='moderated'/>
			<yt:accessControl action='rate' permission='allowed'/>
			<yt:accessControl action='embed' permission='allowed'/>
			<yt:accessControl action='list' permission='allowed'/>
			<yt:accessControl action='autoPlay' permission='allowed'/>
			<yt:accessControl action='syndicate' permission='allowed'/>
			<gd:comments>
				<gd:feedLink rel='https://gdata.youtube.com/schemas/1970#comments' href="{{url}}/api/videos/{{info['videoId']}}/comments?v=2" countHint='5'/>
			</gd:comments>
			<yt:location>Paris ,FR</yt:location>
			<media:group>
				<media:category label='Howto &amp; Style' scheme='https://gdata.youtube.com/schemas/1970/categories.cat'>Howto</media:category>
				<media:content url="https://www.youtube.com/v/{{info['videoId']}}?version=3&amp;f=playlists&amp;app=youtube_gdata" type='application/x-shockwave-flash' medium='video' isDefault='true' expression='full' duration='0' yt:format='5'/>
				<media:content url="{{url}}/getvideo/{{info['videoId']}}" type='video/3gpp' type='video/3gpp' medium='video' expression='full' duration='0' yt:format='1'/>
				<media:content url="{{url}}/getvideo/{{info['videoId']}}" type='video/3gpp' type='video/3gpp' medium='video' expression='full' duration='0' yt:format='6'/>
				<media:credit role='uploader' scheme='urn:youtube' yt:display="{{info['author']}}" yt:type='partner'>{{info['authorId']}}</media:credit>
				<media:description type='plain'>{{info['description']}}</media:description>
				<media:keywords/>
				<media:license type='text/html' href='https://www.youtube.com/t/terms'>youtube</media:license>
				<media:player url="https://www.youtube.com/watch?v={{info['videoId']}}&amp;feature=youtube_gdata_player"/>
				<media:thumbnail url="https://i.ytimg.com/vi/{{info['videoId']}}/default.jpg" height='90' width='120' time='00:00:00.000' yt:name='default'/>
				<media:thumbnail url="https://i.ytimg.com/vi/{{info['videoId']}}/mqdefault.jpg" height='180' width='320' yt:name='mqdefault'/>
				<media:thumbnail url="https://i.ytimg.com/vi/{{info['videoId']}}/mqdefault.jpg" height='360' width='480' yt:name='hqdefault'/>
				<media:thumbnail url="https://i.ytimg.com/vi/{{info['videoId']}}/default.jpg" height='90' width='120' time='00:00:00.000' yt:name='start'/>
				<media:thumbnail url="https://i.ytimg.com/vi/{{info['videoId']}}/default.jpg" height='90' width='120' time='00:00:00.000' yt:name='middle'/>
				<media:thumbnail url="https://i.ytimg.com/vi/{{info['videoId']}}/default.jpg" height='90' width='120' time='00:00:00.000' yt:name='end'/>
				<media:content url="{{url}}/getvideo/{{info['videoId']}}" type="video/mp4" medium="video" isDefault="true" expression="full" duration="0" yt:format="3" />
				<media:content url="{{url}}/getvideo/{{info['videoId']}}" type="video/3gpp" medium="video" expression="full" duration="0" yt:format="2" />
				<media:content url="{{url}}/getvideo/{{info['videoId']}}" type="video/mp4" medium="video" expression="full" duration="0" yt:format="8" />
				<media:content url="{{url}}/getvideo/{{info['videoId']}}" type="video/3gpp" medium="video" expression="full" duration="0" yt:format="9" />
				<media:title type='plain'>{{info['title']}}</media:title>
				<yt:duration seconds="{{info['lengthSeconds']}}"/>
				<yt:uploaded>{{'published' in info and unix(info['published']) or unix('0')}}</yt:uploaded>
				<yt:uploaderId>{{info['authorId']}}</yt:uploaderId>
				<yt:videoid>{{info['videoId']}}</yt:videoid>
			</media:group>
			<gd:rating average='0' max='0' min='0' numRaters='0' rel='https://schemas.google.com/g/2005#overall'/>
			<yt:recorded>1970-08-22</yt:recorded>
			<yt:statistics favoriteCount='0' viewCount="{{info['viewCount']}}"/>
			<yt:rating numDislikes='100' numLikes='10000'/>
			<yt:position>1</yt:position>
		</entry>
	{% endfor %}
{% endif %}