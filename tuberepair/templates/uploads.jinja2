<?xml version='1.0' encoding='UTF-8'?>
<feed xmlns="http://www.w3.org/2005/Atom" xmlns:gd="http://schemas.google.com/g/2005" xmlns:openSearch="http://a9.com/-/spec/opensearch/1.1/" xmlns:yt="{{ url }}/schemas/2007" xmlns:media="http://search.yahoo.com/mrss/" gd:etag="">
  <id>{{ url }}/feeds/api/standardfeeds/US/recently_featured</id>
  <category scheme="http://schemas.google.com/g/2005#kind" term="{{ url }}/schemas/2007/#video" />
<title>Spotlight Videos</title>  <logo>http://www.gstatic.com/youtube/img/logo.png</logo>
  <link rel="http://schemas.google.com/g/2005#feed" type="application/atom+xml" href="{{url}}/feeds/api/standardfeeds/US/recently_featured" />
  <link rel="http://schemas.google.com/g/2005#batch" type="application/atom+xml" href="{{url}}/feeds/api/standardfeeds/US/recently_featured/batch" />
  <link rel="self" type="application/atom+xml" href="{{url}}/feeds/api/standardfeeds/US/recently_featured?start-index=1&amp;max-results=25&amp;format=2,3,8,9&amp;fields=openSearch:totalResults,openSearch:startIndex,openSearch:itemsPerPage,link%5B@rel=&#039;http://schemas.google.com/g/2005%23batch&#039;%5D,entry(id,title,updated,published,yt:rating,link%5B@rel=&#039;edit&#039;%20or%20@rel=&#039;{{url}}/schemas/2007%23video.ratings&#039;%5D,yt:statistics(@viewCount),batch:status,yt:accessControl%5B@action=&#039;list&#039;%5D,media:group(media:thumbnail,media:content%5B@yt:format=&#039;2&#039;%20or%20@yt:format=&#039;3&#039;%20or%20@yt:format=&#039;8&#039;%20or%20@yt:format=&#039;9&#039;%5D(@yt:format,@url,@duration),media:category,media:player,media:description,media:keywords,media:rating,yt:videoid,media:credit,yt:private),app:control,gd:comments)" />
  {% if continuation %}
    <link rel='next' type='application/atom+xml' href='{{url}}/feeds/api/users/UCd_ROizZ_2A63WwnuDmLZGg/uploads?continuation={{continuation}}'/>
  {% endif %}
  <author>
    <name>YouTube</name>
    <uri></uri>
  </author>
<openSearch:totalResults>20</openSearch:totalResults>
  <openSearch:itemsPerPage>25</openSearch:itemsPerPage>
  <openSearch:startIndex>1</openSearch:startIndex>
{% if data %}
  {% for uploads in data %}
      <entry gd:etag="geQ3cverb8elIZNEapXqfu2pOIY">
      <id>tag:youtube.com,2008:video:{{ uploads['videoId'] }}</id>
      <published>{{ unix(uploads['published']) }}</published>
      <updated>{{ unix(uploads['published']) }}</updated>
      <category scheme="http://schemas.google.com/g/2005#kind" term="{{ url }}/schemas/2007#video" />
      <category scheme="{{ url }}/schemas/2007/categories.cat" term="Music" label="Music" />
      <title>{{ uploads['title'] }}</title>
      <content type="video/mp4" src="{{ url }}/getvideo/{{ uploads['videoId'] }}" />
      <link rel="alternate" type="text/html" href="https://www.youtube.com/watch?v={{ uploads['videoId'] }}&amp;feature=youtube_gdata" />
      <link rel="{{ url }}/schemas/2007#video.complaints" type="application/atom+xml" href="{{url}}/feeds/api/videos/{{ uploads['videoId'] }}/complaints" />
      <link rel="{{ url }}/schemas/2007#video.related" type="application/atom+xml" href="{{url}}/feeds/api/videos/{{ uploads['videoId'] }}/related" />
      <link rel="{{ url }}/schemas/2007#video.captionTracks" type="application/atom+xml" href="{{url}}/feeds/api/videos/{{ uploads['videoId'] }}/captions" yt:hasEntries="false" />
      <link rel="{{ url }}/schemas/2007#mobile" type="text/html" href="https://m.youtube.com/details?v={{ uploads['videoId'] }}" />
      <link rel="{{ url }}/schemas/2007#uploader" type="application/atom+xml" href="{{url}}/feeds/api/users/UCealgY8FrRPdAaCN8UtkuIQ" />
      <link rel="self" type="application/atom+xml" href="{{ url }}/getvideo/{{ uploads['videoId'] }}" />
      <link rel='http://gdata.youtube.com/schemas/2007#video.related' type='application/atom+xml' href="https://gdata.youtube.com/feeds/api/videos/{{uploads['videoId']}}/related"/>
      <author>
        <name>{{ uploads['author'] }}</name>
        <uri>{{ url }}/feeds/api/users/{{ uploads['author'] }}</uri>
        <yt:userId>{{ uploads['authorId'] }}</yt:userId>
      </author>

      <yt:accessControl action="comment" permission="allowed" />
      <yt:accessControl action="commentVote" permission="allowed" />
      <yt:accessControl action="videoRespond" permission="denied" />
      <yt:accessControl action="rate" permission="allowed" />
      <yt:accessControl action="embed" permission="allowed" />
      <yt:accessControl action="list" permission="allowed" />
      <yt:accessControl action="monetize" permission="denied" />
      <yt:accessControl action="autoPlay" permission="allowed" />
      <yt:accessControl action="syndicate" permission="allowed" />
      <gd:comments><gd:feedLink href="{{ url }}/api/videos/{{ uploads['videoId'] }}/comments"/></gd:comments>
      <yt:statistics favoriteCount="0" viewCount="{{ uploads['viewCount'] }}" />
  <gd:rating average="3" max="3" min="3" numRaters="1" rel="http://schemas.google.com/g/2005#overall" />
      <yt:rating numDislikes="0" numLikes="0" />
  <yt:hd />
      <media:group>
        <media:category label="Music" scheme="{{ url }}/schemas/2007/categories.cat">Music</media:category>
        <media:content url="{{ url }}/getvideo/{{ uploads['videoId'] }}" type="video/mp4" medium="video" isDefault="true" expression="full" duration="0" yt:format="3" />
        <media:content url="{{ url }}/{{ uploads['videoId'] }}.3gpp" type="video/3gpp" medium="video" expression="full" duration="0" yt:format="2" />
        <media:content url="{{ url }}/getvideo/{{ uploads['videoId'] }}" type="video/mp4" medium="video" expression="full" duration="0" yt:format="8" />
        <media:content url="{{ url }}/{{ uploads['videoId'] }}.3gpp" type="video/3gpp" medium="video" expression="full" duration="0" yt:format="9" />
        <media:credit role="uploader" scheme="urn:youtube" yt:display="{{ uploads['author'] }}">{{ uploads['author'] }}</media:credit>
        <media:description type="plain">{{ uploads['description'] }}</media:description>
        <media:keywords>keywords</media:keywords>
        <media:license type="text/html" href="http://www.youtube.com/t/terms">youtube</media:license>
        <media:player url="http://www.youtube.com/watch?v={{ uploads['videoId'] }}&amp;feature=youtube_gdata_player" />
        <media:thumbnail url="https://i.ytimg.com/vi/{{ uploads['videoId'] }}/default.jpg" height="90" width="120" yt:name="default" />
        <media:thumbnail url="https://i.ytimg.com/vi/{{ uploads['videoId'] }}/mqdefault.jpg" height="180" width="320" yt:name="mqdefault" />
        <media:thumbnail url="https://i.ytimg.com/vi/{{ uploads['videoId'] }}/hqdefault.jpg" height="360" width="480" yt:name="hqdefault" />
        <media:thumbnail url="https://i.ytimg.com/vi/{{ uploads['videoId'] }}/hqdefault.jpg" height="480" width="640" yt:name="sddefault" />
        <media:thumbnail url="https://i.ytimg.com/vi/{{ uploads['videoId'] }}/hqdefault.jpg" height="720" width="1280" yt:name="naxresdefault" />
        <media:title type="plain">{{ uploads['title'] }}</media:title>
        <yt:aspectRatio>widescreen</yt:aspectRatio>
        <yt:duration seconds="{{ uploads['lengthSeconds'] }}" />
        <yt:uploaded>{{ unix(uploads['published']) }}</yt:uploaded>
        <yt:uploaderId>{{ uploads['authorId'] }}</yt:uploaderId>
        <yt:videoid>{{ uploads['videoId'] }}</yt:videoid>
      </media:group>
    </entry>
  {% endfor %}
{% endif %}
</feed>